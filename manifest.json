{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    { "run_at" :"document_end",
    "matches": ["https://registrar-apps.ucdavis.edu/courses/search/index.cfm"],
    "js": ["myScript.js"]
    }],
    "web_accessible_resources": [{
        "resources": ["script.js"],
        "matches": ["<all_urls>"]
      }]
}